{
  "name": "elvish-date",
  "version": "1.0.0",
  "description": "Elvish Calendar conversion of dates and utilities",
  "main": "lib/index.js",
  "scripts": {
    "build:test": "babel test -d lib-test --copy-files",
    "start:test": "mocha lib-test --require babel-core/register",
    "test": "yarn run build:test && yarn run start:test",
    "dist": "webpack",
    "prepublish": "babel src -d lib --copy-files",
    "build": "yarn run dist && yarn run prepublish",
    "start": "yarn run build && node lib",
    "lint": "eslint src test",
    "docs": "jsdoc -d docs src",
    "docs:md": "jsdoc2md src/ElvishDate.js > DOCS.md",
    "release": "yarn build && yarn lint && yarn test && yarn docs && yarn docs:md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquingatica/elvish-date"
  },
  "keywords": [
    "elvish",
    "date",
    "calendar",
    "imladris"
  ],
  "author": "Joaquin Gatica <joaquingatica@gmail.com> (https://about.me/joaquingatica)",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^3.0.4",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {}
}
